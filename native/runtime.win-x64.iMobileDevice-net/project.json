{
  "version": "1.2.1-*",

  "authors": [
    "Quamotion"
  ],

  "title": "Windows (64-bit) Native libraries for .NET client for iOS devices (libimobiledevice-net)",
  "description": ".NET bindings for libimobiledevice, allows you to interact with devices running iOS, such as iPhones or iPads, using C# or VB.NET code",
  "packOptions": {
    "licenseUrl": "https://raw.githubusercontent.com/libimobiledevice-win32/imobiledevice-net/blob/master/LICENSE.txt",
    "owners": [
      "Quamotion"
    ],
    "repository": {
      "type": "git",
      "url": "http://github.com/libimobiledevice-win32/imobiledevice-net"
    },
    "tags": [ "libimobiledevice", "ios", "iphone", "ipad", "imobiledevice", "netcore" ],
    "files": {
      "mappings": {
        "build/net40/": [
          "runtime.win-x64.iMobileDevice-net.targets"
        ],
        "runtimes/win7-x64/native/": [
          "../../packages/libimobiledevice.redist.1.2.1.79/build/native/bin/x64/v140/Release/*.*",
          "../../packages/libusbmuxd.redist.1.0.10.25/build/native/bin/x64/v140/Release/*.*",
          "../../packages/libiconv.redist.1.14.0.11/build/native/bin/v110/x64/Release/dynamic/cdecl/*.dll",
          "../../packages/openssl.v140.windesktop.msvcstl.dyn.rt-dyn.x64.1.0.2.0/lib/native/v140/windesktop/msvcstl/dyn/rt-dyn/x64/release/*.dll",
          "../../packages/zlib.v140.windesktop.msvcstl.dyn.rt-dyn.1.2.8.8/lib/native/v140/windesktop/msvcstl/dyn/rt-dyn/x64/Release/*.dll",
          "../../packages/fix8.dependencies.getopt.1.0.20151130.1/build/native/bin/x64/v140/Release/Desktop/*.dll",
          "../../packages/usbmuxd.1.1.0.50/runtimes/win7-x64/native/*.exe",
          "../../packages/usbmuxd.1.1.0.50/runtimes/win7-x64/native/*.dll",
          "../../packages/pthreads.redist.2.9.1.4/build/native/bin/v110/x64/Release/dynamic/cdecl/*.dll",
          "msvcr110.dll",
          "vcruntime140.dll"
        ],
        "runtimes/win7-x64/native/imobiledevice-net-lighthouse-x64.dll": "lighthouse-x64.dll"
      }
    }
  },

  "frameworks": {
    "netstandard1.5": {
    },
    "net45": {
    }
