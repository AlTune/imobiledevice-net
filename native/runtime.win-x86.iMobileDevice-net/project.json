{
  "version": "1.2.1-*",

  "authors": [
    "Quamotion"
  ],

  "title": "Windows (32-bit) Native libraries for .NET client for iOS devices (libimobiledevice-net)",
  "description": ".NET bindings for libimobiledevice, allows you to interact with devices running iOS, such as iPhones or iPads, using C# or VB.NET code",
  "packOptions": {
    "licenseUrl": "https://raw.githubusercontent.com/libimobiledevice-win32/imobiledevice-net/blob/master/LICENSE.txt",
    "owners": [
      "Quamotion"
    ],
    "repository": {
      "type": "git",
      "url": "http://github.com/libimobiledevice-win32/imobiledevice-net"
    },
    "tags": [ "libimobiledevice", "ios", "iphone", "ipad", "imobiledevice", "netcore" ],
    "files": {
      "mappings": {
        "build/net40/": [
          "runtime.win-x86.iMobileDevice-net.targets"
        ],
        "runtimes/win7-x86/native/": [
          "../../packages/libimobiledevice.redist.1.2.1.79/build/native/bin/Win32/v140/Release/*.*",
          "../../packages/libusbmuxd.redist.1.0.10.25/build/native/bin/Win32/v140/Release/*.*",
          "../../packages/libiconv.redist.1.14.0.11/build/native/bin/v110/Win32/Release/dynamic/cdecl/*.dll",
